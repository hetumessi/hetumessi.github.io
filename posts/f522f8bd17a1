void Swap(int *a,int *b)&#123;
    int temp&#x3D;*a;
    *a&#x3D;*b;
    *b&#x3D;temp;
&#125;
void Swap(float *a,float *b)&#123;
    float temp&#x3D;*a;
    *a&#x3D;*b;
    *b&#x3D;temp;
&#125;
void Swap(char *a,char *b)&#123;
    char temp&#x3D;*a;
    *a&#x3D;*b;
    *b&#x3D;temp;
&#125;
void Swap(bool *a,bool *b)&#123;
    char temp&#x3D;*a;
    *a&#x3D;*b;
    *b&#x3D;temp;
&#125;
int main()&#123;
    int n1&#x3D;100,n2&#x3D;200;
    Swap(&amp;n1,&amp;n2);
    cout&lt;&lt;n1&lt;&lt;&quot;, &quot;&lt;&lt;n2&lt;&lt;endl;
